{
  "task.allowAutomaticTasks": "off",
  "git.mergeEditor": false,
  "terminal.integrated.shellIntegration.enabled": true,
  "workbench.preferredDarkColorTheme": "Andromeda Bordered",
  "workbench.preferredLightColorTheme": "Quiet Light",
  "window.commandCenter": false,
  "workbench.sideBar.location": "left",
  "workbench.statusBar.visible": false,
  "zenMode.hideTabs": true,
  "zenMode.fullScreen": false,
  "zenMode.restore": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 3,
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "workbench.activityBar.visible": false,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": false,
  "workbench.editor.showTabs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.fontFamily": "Martian Mono Std Lt",
  "editor.codeLensFontFamily": "Martian Mono Std Lt",
  "editor.inlayHints.fontFamily": "Martian Mono sWd Th",
  "editor.inlayHints.fontSize": 8,
  "editor.lineHeight": 1.65,
  "terminal.integrated.fontSize": 9,
  "markdown.preview.fontFamily": "Martian Mono Std Rg",
  "security.workspace.trust.untrustedFiles": "open",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "workbench.colorTheme": "Andromeda Bordered",
  "task.problemMatchers.neverPrompt": {
    "shell": true
  },
  "inlineFold.unfoldedOpacity": 0.9,
  "inlineFold.unfoldOnLineSelect": true,
  "inlineFold.maskChar": "â†’",
  "workbench.iconTheme": "city-lights-icons-vsc",
  "workbench.startupEditor": "none",
  "vscode-pets.petSize": "medium",
  "vscode-pets.petColor": "white",
  "vscode-pets.petType": "totoro",
  "editor.inlayHints.enabled": "off",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "errorLens.followCursor": "activeLine",
  "errorLens.delay": 1300,
  "vscode-pets.position": "explorer",
  "vscode-pets.theme": "forest",

  "diffEditor.ignoreTrimWhitespace": false,
  "editor.wordWrap": "on",
  "tailwindCSS.rootFontSize": 14,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  }
}
